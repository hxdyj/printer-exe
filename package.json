{
  "name": "printer-exe",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "author": "songle",
  "description": "test",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "_tsc": "tsc",
    "estart": "npm run _tsc &&  electron .",
    "package": "npm run _tsc && electron-forge package",
    "make": "npm run _tsc && electron-forge make",
    "start": "npm run _tsc && electron-forge start"
  },
  "dependencies": {
    "download": "^8.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa-ts-decorator-router": "^0.1.8",
    "pdf-to-printer": "^5.1.1",
    "uid": "^2.0.0",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/download": "^8.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@vitejs/plugin-vue": "^1.9.3",
    "electron": "^16.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_vue_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
